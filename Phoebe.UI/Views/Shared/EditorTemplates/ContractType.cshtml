@model int?
@using Phoebe.Model

<select id="ContractType" name="ContractType" class="form-control">
    <option value="">-- 请选择 --</option>
    @foreach (ContractType o in Enum.GetValues(typeof(ContractType)))
    {
        <option value="@Convert.ToInt32(o)" @(Model == (int)o ? "selected='selected'" : "")>@o.DisplayName()</option>
    }
</select>
